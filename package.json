{
  "name": "projeto-rdb",
  "version": "1.0.0",
  "description": "projeto de analise de arquivos e geração de automação",
  "main": "src/index.js",
  "scripts": {
    "build-doc": "snowboard html -o docs api.apib",
    "build": "tsc",
    "start": "npm install &&  node --max-old-space-size=4096 dist/ports/http/index.js --unhandled-rejections=strict",
    "dev": " npm install & npm run build & nodemon dist/ports/http/index.js",
    "test": "echo 'no tests defined yet.'",
    "lint": "eslint .",
    "lint:api": "snowboard lint api.apib",
    "eslint": "eslint --no-error-on-unmatched-pattern $(git diff --name-only origin/develop)",
    "debug": "tsc && node dist/ports/http/index.js"
  },
  "author": "Jusan Magno",
  "license": "MIT",
  "dependencies": {
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.9",
    "agenda": "^3.1.0",
    "archiver": "^5.1.0",
    "csv-parse": "^4.14.1",
    "csv-parser": "^3.0.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.0",
    "fast-csv": "^4.3.6",
    "jshint": "^2.12.0",
    "jsonwebtoken": "^8.5.1",
    "line-by-line": "^0.1.6",
    "linebyline": "^1.3.0",
    "mongoose": "^5.10.15",
    "monq": "^0.3.7",
    "nodemailer": "^6.4.17",
    "path": "^0.12.7",
    "request": "^2.88.2",
    "ts-node-dev": "^1.0.0",
    "typedi": "^0.8.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "typescript": "^4.1.3"
  }
}
